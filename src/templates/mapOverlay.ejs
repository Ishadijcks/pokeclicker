<% /* %>

Parameters:
    - name : Required : String
        The name of this POI.

    - x : Required : Number
        The x axis position of this POI element.

    - y : Required : Number
        The y axis position of this POI element.

    - databind : Required : String
        Any knockout data-bind needed for this element to function.

    - image : Required : String
        The image path

    - visible : Required : String
        Visible when true

<% */ %>

<image xlink:href="<%= locals.image %>"
    x="<%= locals.x * 16 %>"
    y="<%= locals.y * 16 %>"
    height="<%= (locals.height || 3) * 16 %>"
    width="<%= (locals.width || 3) * 16 %>"
    data-bind="visible: <%= locals.visible %>"/>
<rect class="city"
    data-town="<%= locals.name %>" 
    x="<%= locals.x * 16 %>"
    y="<%= locals.y * 16 %>"
    height="<%= (locals.height || 3) * 16 %>"
    width="<%= (locals.width || 3) * 16 %>"
    data-bind="visible: <%= locals.visible %>, <%= locals.databind %>">
</rect>